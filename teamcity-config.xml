<?xml version="1.0" encoding="UTF-8"?>
<build-type xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" uuid="FoxkitTests" xsi:noNamespaceSchemaLocation="https://www.jetbrains.com/teamcity/schemas/2021.1/project-config.xsd">
  <name>Foxkit Tests</name>
  <description>Runs tests for the Foxkit project</description>
  <settings>
    <parameters>
      <param name="env.PSModulePath" value="" />
    </parameters>
    <build-runners>
      <runner id="RUNNER_1" name="Set PowerShell Execution Policy" type="simpleRunner">
        <parameters>
          <param name="script.content" value="powershell -Command &quot;Set-ExecutionPolicy -ExecutionPolicy Bypass -Scope Process&quot;" />
          <param name="teamcity.step.mode" value="default" />
          <param name="use.custom.script" value="true" />
        </parameters>
      </runner>
      <runner id="RUNNER_2" name="Run Foxkit Tests" type="simpleRunner">
        <parameters>
          <param name="script.content" value="powershell -File teamcity-test.ps1" />
          <param name="teamcity.step.mode" value="default" />
          <param name="use.custom.script" value="true" />
        </parameters>
      </runner>
    </build-runners>
    <vcs-settings>
      <vcs-entry-ref root-id="FoxkitVcsRoot" />
    </vcs-settings>
    <requirements>
      <equals name="teamcity.agent.jvm.os.name" value="Windows" />
    </requirements>
    <build-triggers>
      <build-trigger id="TRIGGER_1" type="vcsTrigger">
        <parameters>
          <param name="branchFilter" value="+:*" />
          <param name="enableQueueOptimization" value="true" />
          <param name="quietPeriodMode" value="DO_NOT_USE" />
        </parameters>
      </build-trigger>
    </build-triggers>
    <cleanup />
  </settings>
</build-type>
